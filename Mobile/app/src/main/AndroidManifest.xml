<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.interdisciplinar.mobile" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GrupoActivity"
            android:label="@string/title_activity_grupo"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".GrupoActivityForm"
            android:label="@string/title_activity_activity_grupo_form"
            android:parentActivityName=".GrupoActivity" >
        </activity>
        <activity
            android:name=".NegocioActivity"
            android:label="@string/title_activity_negocio"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".NegocioActivityForm"
            android:label="@string/title_activity_negocio_form"
            android:parentActivityName=".NegocioActivity" >
        </activity>
        <activity
            android:name=".ProdutoActivityForm"
            android:label="@string/title_activity_produto_activity_form"
            android:parentActivityName=".ProdutoActivity" >
        </activity>
        <activity
            android:name=".NegocioActivityDetalhes"
            android:label="@string/title_activity_negocio_activity_details"
            android:parentActivityName=".NegocioActivity" >
        </activity>
        <activity
            android:name=".ProdutoActivityDetalhes"
            android:label="@string/title_activity_produto" >
        </activity>
        <activity
            android:name=".MateriaisActivity"
            android:label="@string/title_activity_materiais"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".MateriaisActivityForm"
            android:label="@string/title_activity_materiais_activity_form"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".MateriaisProdutoActivity"
            android:label="@string/title_activity_materiais_produto"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".ProdutoActivity"
            android:label="@string/title_activity_produto"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".ProdutoMaterialActivity"
            android:label="@string/title_activity_produto_material"
            android:parentActivityName=".ProdutoActivityDetalhes"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.interdisciplinar.mobile.ProdutoActivityDetalhes" />
        </activity>
        <activity
            android:name=".ProdutoRegistroActivity"
            android:label="@string/title_activity_produto_registro"
            android:parentActivityName=".ProdutoActivityDetalhes"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.interdisciplinar.mobile.ProdutoActivityDetalhes" />
        </activity>
    </application>

</manifest>
